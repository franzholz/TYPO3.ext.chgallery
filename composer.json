{
  "name": "jambagecom/chgallery",
  "type": "typo3-cms-extension",
  "description": "simple gallery, show all images of a directory",
  "keywords": [
    "TYPO3",
    "extension",
    "gallery",
    "pagebrowser",
    "lightbox",
    "ratings"
  ],
  "support": {
    "issues": "https://jambage.com/kontakt/forum.html"
  },
  "homepage": "https://jambage.com",
  "authors": [
    {
        "name": "Georg Ringer",
        "role": "Developer"
    },
    {
      "name": "Franz Holzinger",
      "role": "Developer",
      "homepage": "https://ttproducts.de"
    }
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "php": "^8.1",
    "typo3/cms-core": "^12.4",
    "jambagecom/div2007": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "JambageCom\\Chgallery\\": "Classes"
    },
    "classmap": [
      "pi1"
    ]
  },
  "replace": {
    "typo3-ter/chgallery": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "chgallery"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.46"
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
